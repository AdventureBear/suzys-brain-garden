---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - nav
    - post
    - posts
    - page
    - pages
    - tagList
    - menuItems
    - fragments
    - builds
permalink: /manifest/{{ tag | slugify }}/
layout: layouts/page.njk
eleventyComputed:
  title: "<{{ tag }} />"
---
<pre style="background: transparent; border: none; padding: 0; margin: 0; font-family: 'Courier New', Courier, monospace; font-size: inherit; color: var(--color-text); white-space: pre-wrap;">
{{ collections[tag] | length }} item{% if collections[tag] | length != 1 %}s{% endif %} logged under this heading.
</pre>
<hr />
<div class="space-y-1 mt-8">
{% for post in collections[tag] | reverse %}
<div><a href="{{ post.url }}">{{ post.data.title }}</a></div>
{% endfor %}
</div>

<div class="mt-8">
<a href="/manifest/" style="color: var(--color-text-secondary);">‚Üê back to manifest</a>
</div>
